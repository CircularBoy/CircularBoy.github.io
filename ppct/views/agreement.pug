include inc/var.pug
include inc/mixins.pug
doctype html
html(lang='ru' dir='ltr')
  head
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0')
    block meta
      meta(name='description', content= meta.description)
      meta(itemprop='name' content= meta.title)
      meta(itemprop='description' content= meta.description)
      meta(itemprop='image' content= meta.image)
      meta(property='og:title' content= meta.title)
      meta(property='og:site_name' content= meta.title)
      meta(property='og:url' content= meta.url)
      meta(property='og:description' content= meta.description)
      meta(property='og:image' content= meta.image)
    block title
      title RW Service
    block head
      link(href='styles/libs.min.css' rel='stylesheet')
      link(href='https://fonts.googleapis.com/css?family=Cardo:400,700|Montserrat:400,500' rel='stylesheet')
      link(href='styles/main.css?rev=@@hash' rel='stylesheet')
      link(rel='shortcut icon' href='images/icons/favicon.png' type='image/png')

  body.agreement__page

    header.header.header--fixed
      .header__container.container
        a.header__logo(href="/")

        a.header__btn.btn.btn--outline(href="/") Вернуться назад

    include blocks/agreement.pug
    include blocks/footer.pug


    script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js')
    script(type='text/javascript' src='scripts/libs.min.js')
    script(type='text/javascript' src='scripts/main.js?rev=@@hash')